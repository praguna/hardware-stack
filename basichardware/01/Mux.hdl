/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=ns);
    And(a=a,b=ns,out=f1);
    And(a=b,b=sel,out=f2);
    Not(in=f1,out=p1);
    Not(in=f2,out=p2);
    Nand(a=p1,b=p2,out=out); 
}